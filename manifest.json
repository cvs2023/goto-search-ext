{
  "manifest_version": 3,
  "name": "Google Search Extension",
  "version": "1.0",
  "description": "Search a specific text in Google",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
